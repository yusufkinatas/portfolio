{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "rangeStart": 0,
  "rangeEnd": 10000,
  "parser": "babel",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": ["*.scss"],
      "options": {
        "parser": "scss"
      }
    }
  ]
}
